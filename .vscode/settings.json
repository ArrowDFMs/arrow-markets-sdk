"editor.codeActionsOnSave": {
    "source.fixAll.eslint": {
        "fix": true,
        "fixTypes": [
            "problem",
            "suggestion",
            "layout"
        ],
        "overrideConfig": {
            "rules": {
                "@typescript-eslint/no-unused-vars": "off",
                "no-unused-vars": "off",
                "no-unused-vars-experimental": "off",
                "import/no-unused-modules": "off",
                "no-undef": "off",
                "no-use-before-define": "off"
            }
        }
    }
},
